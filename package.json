  },
  "devDependencies": {
    "@testing-library/react": "^13.3.0",
    "@vitejs/plugin-react": "^2.0.0",
    "@vitest/coverage-c8": "^0.25.4",
    "@vitest/ui": "^0.25.4",
    "cypress": "^11.0.1",
